{
  "extends": "@nuxt/typescript",
  "compilerOptions": {
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "types": [
      "@types/node",
      "@nuxt/vue-app",
    ],
    "module": "esnext",
    "target": "es6",
    "lib": [
      "esnext",
      "dom"
    ],
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    }
  },
}
